<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FDF0F4"/>
            <corners android:radius="5dp" />
            <stroke
                android:width="1dp"
                android:dashWidth="3dp"
                android:color="#FDA39C"
                android:dashGap="3dp"
                />
        </shape>
    </item>
</selector>